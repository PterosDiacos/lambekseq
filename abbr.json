    "rl":     ["(np\\$np)/(s^np)"],
    "gap":    ["(s^np)!$(s^np)"],

    "inn":    ["(np\\$np)/np"],
    "ins":    ["(s\\$s)/np"],
    "inv":    ["(vp\\$vp)/np"],
    "inx":    ["inv", "ins"],

    "neg":    ["(s^(vp/$vp))!s", "vp/vp"],

    "vp":     ["np\\s"],
    "vt":     ["vp/np"],
    "snp":    ["s/np"],
    "svp":    ["s/vp"],

    "conj":   ["(np\\&np)/&np", 
               "(qp\\&qp)/&qp",
               "(vp\\&vp)/&vp",
               "(vt\\&vt)/&vt",
               "(snp\\&snp)/&snp",
               "((svp/np)\\&(svp/np))/&(svp/np)"]
